
public class TesteBanco {

	public static void main(String[] args) {
		Pessoa p = new Pessoa("Goku", 123, 10);
		System.out.println(
				"Nome: " + p.exibirNome() +
				"\nCPF: " + p.exibirCpf() +
				"\nIdade: " + p.exibirIdade()
 				);
		
		Conta c = new Conta();
		c.setTitular(p);
		Pessoa x = c.getTitular();
		System.out.println("Titular: " + x.exibirNome());
	}
}











