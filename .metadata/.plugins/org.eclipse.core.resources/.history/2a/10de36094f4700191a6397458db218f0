
public class Conta {
	private String tipo;
	private Pessoa titular;
	
	public Conta(String pTipo, Pessoa pTitular) {
		tipo = pTipo;
		titular = pTitular;
	}
	
	public void setTitular(Pessoa pTitular) {
		titular = pTitular;
	}
	
	public Pessoa getTitular() {
		return titular;
	}
	
	public void setTipo(String pTipo) {
		tipo = pTipo;
	}
	
	public String getTipo() {
		return tipo;
	}
}
