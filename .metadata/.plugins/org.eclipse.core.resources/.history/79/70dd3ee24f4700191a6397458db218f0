
public class TesteBanco {

	public static void main(String[] args) {
		Pessoa p = new Pessoa("Goku", 123, 10);
		System.out.println(
				"Nome: " + p.exibirNome() +
				"\nCPF: " + p.exibirCpf() +
				"\nIdade: " + p.exibirIdade()
 				);
		
		System.out.println("-------------------------------------------------------");
		Conta c = new Conta("Conta Corrente",p);
		Pessoa x = c.getTitular();
		System.out.println(
				"Tipo de Conta: " + c.getTipo() +
				"\nTitular: " + x.exibirNome()
				);
	}
}











