
public class TesteBanco {

	public static void main(String[] args) {
		Pessoa p = new Pessoa("Goku", 123, 10);
		System.out.println(
				"Nome: " + p.exibirNome() +
				"\nCPF: " + p.exibirCpf() +
				"\nIdade: " + p.exibirIdade()
 				);
		
		System.out.println("-------------------------------------------------------");
		Conta c = new Conta("Corrente",p, "senha", 123);
		c.setSaldo(5000.00);
		Pessoa x = c.getTitular();
		System.out.println(
				"Tipo de Conta: " + c.getTipo() +
				"\nTitular: " + x.exibirNome() +
				"\nSenha: " + c.getSenha() +
				"\nCodigo: " + c.getCodigo() +
				"\nSaldo: " + c.getSaldo()
				);
	}
}











